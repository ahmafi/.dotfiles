{
	"handler": {
		"prefix": "handler",
		"body": [
			"package ${TM_DIRECTORY/^.+\\/(.*)$/$1/}",
			"",
			"import \"context\"",
			"",
			"type ${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/}Input struct {",
			"\tBody struct {",
			"\t}",
			"}",
			"",
			"type ${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/}OutputBody struct {",
			"\tMessage string `json:\"message,omitempty\"`",
			"}",
			"",
			"type ${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/}Output struct {",
			"\tBody ${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/}OutputBody",
			"}",
			"",
			"func (s *Service) ${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/}(ctx context.Context, input *${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/}Input) (*${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/}Output, error) {",
			"\tpanic(\"not implemented\") // TODO: implement",
			"}"
		]
	}
}
